{
    "env": {
        "node": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "object-curly-spacing": ["error", "never"],
        "no-control-regex": "off",
        "no-useless-escape": "off"
    }
}
